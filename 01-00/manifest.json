{
	"name": "My first extension",
	"version": "1.0.0",
	"description": "This is my very first coll extension",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["https://*.youtube.com/*"],
			"js": ["content.js"]
		}
	],
	"permissions": [
		"bookmarks"
	]
}